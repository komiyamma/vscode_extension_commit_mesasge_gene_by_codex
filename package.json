{
  "publisher": "komiyamma",
  "name": "commit-message-gene-by-codex",
  "displayName": "commit-message-gene-by-codex",
  "description": "Generates commit messages in codex environments. It is intended for use when 'Github Copilot' is no longer available or with 'AWS kiro'.",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/komiyamma/vscode_extension_commit_message_gene_by_codex"
  },
  "license": "MIT",
  "homepage": "https://komiyamma.net/?page=page-vscode-extension-commit-message-gene-by-codex",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "commit-message-gene-by-codex.runCodexCmd",
        "title": "Commit message generation command execution"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
